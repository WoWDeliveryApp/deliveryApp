			= image_tag 'ToGOjp.png'
		.col-xs-12.col-md-8
			%p ToGoDelivery.com
	.col-xs-12.col-md-4.menu
		.col-xs-12.col-md-6
			%a(href="#" title="Fast Food")
				%p ¿Hambre?
				%p ¡Comida a Domicilio!
		.col-xs-12.col-md-6
			%a(href="#" title="How it Works?")
				%p ¿Necesita Ayuda?
				%p ¿como funciona?
	.col-xs-12.col-md-3.menu
		= link_to 'Login', new_user_session_path
		= link_to 'Logout', destroy_user_session_path, method: :delete if current_user
